{
  "name": "@bluesialia/express-graphql",
  "version": "0.12.0",
  "description": "Production ready GraphQL HTTP middleware.",
  "author": "Jorge Domínguez Arnáez (BlueSialia) <bluesialia@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/bluesialia/express-graphql",
  "bugs": {
    "url": "https://github.com/bluesialia/express-graphql/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bluesialia/express-graphql.git"
  },
  "keywords": [
    "express",
    "http",
    "graphql",
    "middleware",
    "api"
  ],
  "engines": {
    "node": ">= 14.x"
  },
  "scripts": {
    "preversion": ". ./resources/checkgit.sh && npm ci",
    "version": "npm test",
    "test": "npm run cspell && npm run lint && npm run prettier:check && npm run build && npm run test:integrations && npm run test:units",
    "test:integrations": "mocha --full-trace integrationTests/index.js",
    "test:units": "mocha --inspect dist/**/__tests__/**/*.js",
    "cspell": "cspell '**/*'",
    "lint": "eslint src",
    "prettier": "prettier --write --list-different .",
    "prettier:check": "prettier --check .",
    "build": "ttsc",
    "build:check": "ttsc --noEmit",
    "start": "node --inspect dist/examples/index.js",
    "start:subscription": "node --inspect dist/examples/index_subscription.js"
  },
  "dependencies": {
    "content-type": "~1.0.4",
    "http-errors": "~2.0.0"
  },
  "devDependencies": {
    "@graphiql/toolkit": "~0.6.0",
    "@types/body-parser": "~1.19.2",
    "@types/chai": "~4.3.1",
    "@types/content-type": "~1.1.5",
    "@types/express": "~4.17.13",
    "@types/http-errors": "~1.8.2",
    "@types/mocha": "~9.1.1",
    "@types/multer": "~1.4.7",
    "@types/node": "~18.0.0",
    "@types/sinon": "~10.0.11",
    "@types/supertest": "~2.0.12",
    "@types/ws": "~8.5.3",
    "@typescript-eslint/eslint-plugin": "~5.29.0",
    "@typescript-eslint/parser": "~5.29.0",
    "body-parser": "~1.20.0",
    "chai": "~4.3.6",
    "cspell": "~6.1.2",
    "eslint": "~8.18.0",
    "express": "~4.18.1",
    "graphiql": "~1.9.9",
    "graphql": "~16.5.0",
    "graphql-ws": "~5.9.0",
    "mocha": "~10.0.0",
    "multer": "~1.4.5-lts.1",
    "prettier": "~2.7.1",
    "promise-polyfill": "~8.2.3",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "sinon": "~14.0.0",
    "supertest": "~6.2.3",
    "ts-node": "~10.8.1",
    "ttypescript": "~1.5.13",
    "typescript": "~4.7.4",
    "unfetch": "~4.2.0",
    "ws": "~8.8.0"
  },
  "peerDependencies": {
    "express": "^4.0.0",
    "graphql": "^16.0.0"
  }
}
